<?xml version="1.0"?>
<package>
  <metadata>
    <id>Nancy.ReSharper</id>
    <version>$version$</version>
    <authors>Igal Tabachnik</authors>
    <owners>Igal Tabachnik</owners>
    <title>ReSharper support for NancyFx</title>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://raw.github.com/NancyFx/Nancy.ReSharper/master/license.txt</licenseUrl>
    <description>Nancy plugin for ReSharper adds navigation and intellisense support for NancyFx (http://nancyfx.org) in ReSharper, for super-duper-happy-path web development!</description>
    <projectUrl>https://github.com/NancyFx/Nancy.ReSharper</projectUrl>
    <iconUrl>https://raw.github.com/NancyFx/Nancy.ReSharper/master/nancy.png</iconUrl>
    <copyright>Copyright &#x00A9; 2013 Igal Tabachnik</copyright>
    <tags>nancy nancyfx ssdp super duper happy path simple</tags>
    <releaseNotes>Fixed an issue where MVC Razor views would be affected by the plugin</releaseNotes>
    <dependencies>
      <dependency id="Resharper" version="[8.1,8.2)"/>
    </dependencies>
  </metadata>
  <files>
    <file src="..\output\$configuration$\8.1\*.*" target="ReSharper\v8.1\plugins"/>
    <file src="ExternalAnnotations\**\*.xml" target="ReSharper\vAny\annotations"/>
  </files>
</package>
