    <copyright>Copyright &#x00A9; 2013 Igal Tabachnik</copyright>
    <tags>nancy nancyfx ssdp super duper happy path simple</tags>
    <dependencies>
      <dependency id="Resharper" version="8.0"/>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\output\$configuration$\8.0\*.*" target="ReSharper\v8.0\plugins\NancyFx"/>
  </files>
</package>