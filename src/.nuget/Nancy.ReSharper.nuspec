<?xml version="1.0"?>
<package>
  <metadata>
    <id>Nancy.ReSharper</id>
    <version>$version$</version>
    <authors>Igal Tabachnik</authors>
    <owners>Igal Tabachnik</owners>
    <title>ReSharper support for NancyFx</title>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://raw.github.com/NancyFx/Nancy/master/license.txt</licenseUrl>
